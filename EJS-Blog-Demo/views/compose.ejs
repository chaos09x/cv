<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post" class="item">
    <div class="form-group">
        <label for="">Title</label>
        <input type="text" class="form-control" name="postTitle" placeholder="Blog Title" autocomplete="off">
        <label for="">Post</label>
        <textarea class="form-control" cols="40" rows="5" name="postBody" placeholder="Blog Text" autocomplete="off"></textarea>     
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include("partials/footer") -%>